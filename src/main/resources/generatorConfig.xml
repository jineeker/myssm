<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
		PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_3_0.dtd">
<generatorConfiguration>
	<classPathEntry
			location="F:/maven/mavenRepository/mysql/mysql-connector-java/5.1.38/mysql-connector-java-5.1.38.jar"/>
	<context id="my" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressDate" value="false"/>
			<property name="suppressAllComments" value="true"/>
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
						connectionURL="jdbc:mysql://localhost:3306/cgke" userId="root"
						password="123456"/>

		<javaModelGenerator targetPackage="com.jk.model"
							targetProject="E:/idea/someDemo/myssm/src">
			<property name="enableSubPackages" value="true"/>
			<property name="trimStrings" value="true"/>
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.jk.mapping"
						 targetProject="E:/idea/someDemo/myssm/src">
			<property name="enableSubPackages" value="true"/>
		</sqlMapGenerator>

		<javaClientGenerator targetPackage="com.jk.dao"
							 targetProject="E:/idea/someDemo/myssm/src" type="XMLMAPPER">
			<property name="enableSubPackages" value="true"/>
		</javaClientGenerator>

		<table tableName="users" domainObjectName="Users"
			   enableCountByExample="false" enableUpdateByExample="false"
			   enableDeleteByExample="false" enableSelectByExample="false"
			   selectByExampleQueryId="false">
		</table>

	</context>
</generatorConfiguration>